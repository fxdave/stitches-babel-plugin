  "peerDependencies": {
    "@babel/core": "^7.0.0-0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fxdave/stitches-babel-plugin/issues"
  },
  "homepage": "https://github.com/fxdave/stitches-babel-plugin#readme",
