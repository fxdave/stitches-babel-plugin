    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "babel-plugin",
    "styled",
    "stitches",
    "react",
    "classNames"
  ],
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.10.4"
